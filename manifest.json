{
  "manifest_version": 2,

  "name": "Native",
  "description": "Import articles to Native and split by paragraph",
  "version": "1.0.1",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Importing to Native..."
  },
  "permissions": [
    "tabs",
    "https://getnative.me/" 
  ]
}